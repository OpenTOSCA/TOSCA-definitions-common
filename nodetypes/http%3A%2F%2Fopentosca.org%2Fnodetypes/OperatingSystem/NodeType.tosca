<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<Definitions targetNamespace="http://opentosca.org/nodetypes" id="winery-defs-for_nodetypes-OperatingSystem" xmlns="http://docs.oasis-open.org/tosca/ns/2011/12" xmlns:winery="http://www.opentosca.org/winery/extensions/tosca/2013/02/12" >
    <NodeType name="OperatingSystem" abstract="yes" final="no" targetNamespace="http://opentosca.org/nodetypes" winery:bordercolor="#0000ff">
        <DerivedFrom typeRef="ToscaNormativeNodeTypes:Compute" xmlns:ToscaNormativeNodeTypes="http://docs.oasis-open.org/tosca/ToscaNormativeTypes/nodetypes"/>
        <winery:PropertiesDefinition elementname="properties" namespace="http://opentosca.org/nodetypes/propertiesdefinition/winery">
            <winery:properties>
                <winery:key>instanceRef</winery:key>
                <winery:type>xsd:string</winery:type>
            </winery:properties>
            <winery:properties>
                <winery:key>VMIP</winery:key>
                <winery:type>xsd:string</winery:type>
            </winery:properties>
        </winery:PropertiesDefinition>
        <Interfaces>
            <Interface name="OperatingSystemInterface">
                <Operation name="transferFile">
                    <InputParameters>
                        <InputParameter name="VMIP" type="xsd:String" required="false"/>
                        <InputParameter name="VMUserName" type="xsd:String" required="false"/>
                        <InputParameter name="VMPrivateKey" type="xsd:String" required="false"/>
                        <InputParameter name="TargetAbsolutePath" type="xsd:String" required="false"/>
                        <InputParameter name="SourceURLorLocalPath" type="xsd:String" required="false"/>
                    </InputParameters>
                    <OutputParameters>
                        <OutputParameter name="TransferResult" type="xsd:String" required="false"/>
                    </OutputParameters>
                </Operation>
                <Operation name="runScript">
                    <InputParameters>
                        <InputParameter name="VMIP" type="xsd:String" required="false"/>
                        <InputParameter name="VMUserName" type="xsd:String" required="false"/>
                        <InputParameter name="VMPrivateKey" type="xsd:String" required="false"/>
                        <InputParameter name="Script" type="xsd:String" required="false"/>
                    </InputParameters>
                    <OutputParameters>
                        <OutputParameter name="ScriptResult" type="xsd:String" required="false"/>
                    </OutputParameters>
                </Operation>
                <Operation name="installPackage">
                    <InputParameters>
                        <InputParameter name="VMIP" type="xsd:String" required="false"/>
                        <InputParameter name="VMUserName" type="xsd:String" required="false"/>
                        <InputParameter name="VMPrivateKey" type="xsd:String" required="false"/>
                        <InputParameter name="PackageNames" type="xsd:String" required="false"/>
                    </InputParameters>
                    <OutputParameters>
                        <OutputParameter name="InstallResult" type="xsd:String" required="false"/>
                    </OutputParameters>
                </Operation>
                <Operation name="waitForAvailability">
                    <InputParameters>
                        <InputParameter name="VMIP" type="xsd:String" required="false"/>
                        <InputParameter name="VMUserName" type="xsd:String" required="false"/>
                        <InputParameter name="VMPrivateKey" type="xsd:String" required="false"/>
                    </InputParameters>
                    <OutputParameters>
                        <OutputParameter name="WaitResult" type="xsd:String" required="false"/>
                    </OutputParameters>
                </Operation>
            </Interface>
        </Interfaces>
    </NodeType>
</Definitions>
